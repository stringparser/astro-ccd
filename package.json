{
  "name": "astro-ccd",
  "version": "1.0.0",
  "description": "A very simple example of Next.js-based website with Jest tests, all written in TypeScript",
  "scripts": {
    "predev": "npm run cache",
    "dev": "next",
    "test": "jest",
    "prebuild": "npm run cache",
    "build": "next build",
    "precache": "rm -r cache && mkdir cache",
    "cache": "ts-node ./bin/cache.ts",
    "start": "next start",
    "export": "next export",
    "ts:node": "ts-node -r tsconfig-paths/register",
    "prefetch:data": "rm -f ./src/pages/registro/*.mdx && rm -rf debug && mkdir debug",
    "fetch:data": "npm run ts:node ./bin/fetchData.ts",
    "postfetch:data": "npm run cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stringparser/astro-ccd.git"
  },
  "author": "Jun Kato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stringparser/astro-ccd/issues"
  },
  "homepage": "https://github.com/stringparser/astro-ccd#readme",
  "dependencies": {
    "@babel/core": "7.12.9",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.4",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "image-size": "^0.9.3",
    "material-ui": "^0.20.2",
    "next": "^10.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scroll-up": "^1.3.7",
    "remark": "^13.0.0",
    "remark-mdx": "^1.6.22",
    "safe-regex": "^2.1.1",
    "to-vfile": "^6.1.0",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/file-loader": "^4.2.0",
    "@types/fs-extra": "^9.0.6",
    "@types/jest": "^26.0.13",
    "@types/js-beautify": "^1.13.1",
    "@types/json5": "^0.0.30",
    "@types/material-ui": "^0.20.8",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/safe-regex": "^1.1.2",
    "cheerio": "^1.0.0-rc.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "fs-extra": "^9.0.1",
    "gray-matter": "^4.0.2",
    "jest": "^26.4.2",
    "next-mdx-remote": "^2.1.2",
    "node-fetch": "^2.6.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    }
  }
}