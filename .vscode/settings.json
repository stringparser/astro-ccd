{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.options": {
    "extensions": [
      ".js",
      ".jsx",
      ".md",
      ".mdx",
      ".ts",
      ".tsx"
    ]
  },
  "javascript.preferences.importModuleSpecifier": "non-relative"
}